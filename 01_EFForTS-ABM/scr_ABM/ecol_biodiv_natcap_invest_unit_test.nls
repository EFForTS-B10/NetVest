to natcap-invest-file-copy-tif [file]
  print ["Copying files from input to output folder"]
    let file_in   (word  workdir_natcap "/input/" file ".asc")
    print file_in
    let file_out   (word  workdir_natcap "/output/" file ".asc")
    print file_out
    let file_conv  (word  workdir_natcap "/output/" file)

    py:set "file_in" file_in
    py:set "file_out" file_out
    py:run "from shutil import copyfile"
    py:run "copyfile(file_in, file_out)"
    print (word "copied to file_out:" file_out)
end

