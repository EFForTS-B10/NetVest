
to init-biodiversity
  
  ;; Check biodiversity initialization
  if (biodiv_natcap_invest = "habitatquality") [natcap-invest-habitatquality-init 
                                                natcap-invest-habitatquality-update]
end


to run-biodiversity
  
  if (ticks mod ecol_biodiv_interval = 0)
  [
    ;; Check biodiversity:
    if (biodiv_natcap_invest = "habitatquality") [natcap-invest-habitatquality-update]
  ]
  
end